---

const {img} = Astro.props;

---

<img src={img.src} alt="" />
  
<!-- objectFit: 'cover' -->

<style>
    
    img{
        height:400px;
    }

    /*for mobile*/
    @media only screen and (max-width:620px){
        img{
            height:300px;
            width:auto;
        }
    }
    
</style>